module subtractor (
    input logic [31:0] a, b,
    output logic [31:0] result,
    output logic carry, overflow
);

    logic [32:0] extended_result;

    always_comb begin
        extended_result = a - b;
        result = extended_result[31:0];
        carry = ~extended_result[32];
        overflow = (a[31] != b[31]) && (result[31] != a[31]);
    end

endmodule
